{
  "name": "jensmetzner.github.io",
  "version": "1.0.0",
  "description": "My personal website.",
  "main": "index.js",
  "devDependencies": {
    "npm-run-all": "4.1.3",
    "primer": "10.8.1",
    "pug-cli": "^1.0.0-alpha6",
    "purify-css": "1.2.5",
    "sass": "^1.13.0"
  },
  "scripts": {
    "test": "exit 0",
    "build:pug": "./node_modules/.bin/pug --obj ./src/data.json --out public src",
    "build:css": "./node_modules/.bin/sass src:public --style compressed --load-path ./node_modules/ --no-source-map",
    "build:compress:normal": "./node_modules/.bin/purifycss public/index.css public/index.html -i -m -o public/index.css",
    "build:compress:amp": "./node_modules/.bin/purifycss public/index.amp.css public/index.amp.html -i -m -o public/index.amp.css",
    "build": "./node_modules/.bin/npm-run-all build:css build:pug build:compress:** build:pug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JensMetzner/jensmetzner.github.io.git"
  },
  "keywords": [
    "personal"
  ],
  "author": "Jens Metzner",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JensMetzner/jensmetzner.github.io/issues"
  },
  "homepage": "https://github.com/JensMetzner/jensmetzner.github.io#readme",
  "dependencies": {}
}
